#!/bin/bash
source /usr/local/bin/config_env

PRE=""
CELERY_QUEUES=${CELERY_QUEUES:-celery}
CMD="python ${MANAGE_SCRIPT} celery worker -Q $CELERY_QUEUES --scheduler=djcelery.schedulers.DatabaseScheduler --loglevel=INFO -E $CELERY_OPTS"

if [ ! -z "$NEW_RELIC_LICENSE_KEY" ] ; then
    pip install -U newrelic
    PRE="newrelic-admin run-program "
fi

echo "Starting Worker..."
echo $PRE $CMD
export PYTHONPATH=${AMARAPYTHONPATH}
exec $PRE $CMD
